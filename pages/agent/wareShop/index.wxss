/**pages/agent/wareShop/index**/
.storeInfo {
    display: flex;
    margin-bottom: 14rpx;
    padding: 28rpx 30rpx 32rpx 30rpx;
    background: #ffffff;
    justify-content: flex-start;
    align-items: center;
}
.imgInfo {
    width: 96rpx;
    height: 96rpx;
    margin-right: 20rpx;
}
.imgInfo image {
    display: block;
    width: 100%;
    height: 100%;
    border-radius: 16rpx;
}
.storeDataInfo {
    display: flex;
    width: 416rpx;
    height: 96rpx;
    justify-content: flex-start;
    align-items: center;
}
.storeDataInfo > view {
    display: flex;
    width: 412rpx;
    height: 100%;
    flex-direction: column;
    justify-content: center;
}
.storeName {
    display: block;
    width: 100%;
    font-size: 28rpx;
    color: #333333;
}
.storeData {
    display: flex;
    margin-top: 16rpx;
    justify-content: flex-start;
    align-items: center;
}
.storeDataBlock {
    display: flex;
    margin-right: 20rpx;
    font-size: 22rpx;
    color: #666666;
    justify-content: flex-start;
    align-items: center;
}
.storeDataBlock image {
    display: block;
    width: 32rpx;
    height: 32rpx;
    margin-right: 6rpx;
}

.classify {
    display: flex;
    width: 690rpx;
    height: 104rpx;
    padding: 0 30rpx;
    margin-bottom: 16rpx;
    background: #ffffff;
    justify-content: space-between;
    align-items: center;
}
.classify > text {
    display: block;
    font-size: 32rpx;
    color: #333333;
}
.classifyElement {
    display: flex;
    height: 100%;
    justify-content: flex-start;
    align-items: center;
}
.classifyElement text {
    display: block;
    margin-right: 10rpx;
    font-size: 28rpx;
    color: #999999;
}
.classifyElement image {
    display: block;
    width: 12rpx;
    height: 22rpx;
}

.screenWare {
    display: flex;
    width: 100%;
    height: 100rpx;
    background: #ffffff;
    border-bottom: 2rpx solid #F2F2F2;
    justify-content: space-around;
    align-items: center;
}
.everyScreen {
    display: flex;
    justify-content:flex-start;
    align-items: center;
}
.everyScreen text {
    display: block;
    margin-right: 4rpx;
    font-size: 28rpx;
    color: #333333;
}
.everyScreenState {
    display: flex;
    flex-direction: column;
}
.everyScreenState image {
    display: block;
    width: 12rpx;
    height: 12rpx;
}
.everyScreenState image:first-child {
    margin-bottom: 4rpx;
}

.wareListElement {
    width: 100%;
    height: auto;
    background: #ffffff;
}
.everyWare {
    width: 690rpx;
    padding: 30rpx 30rpx 34rpx 30rpx;
    border-bottom: 2rpx solid #F2F2F2;
}
.everyWareInfo {
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.everySelect {
    width: 28rpx;
    height: 28rpx;
    border-radius: 50%;
}
.everySelect image {
    display: block;
    width: 100%;
    height: 100%;
}
.everyWareInfo > image {
    display: block;
    width: 168rpx;
    height: 168rpx;
    border-radius: 16rpx;
}
.everyDescribe {
    width: 454rpx;
}
.everyTitle {
    overflow: hidden;
    display: -webkit-box;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #333333;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    text-overflow: ellipsis;
}
.everyPrice {
    display: flex;
    margin-top: 2rpx;
    justify-content: flex-start;
    align-items: center;
}
.everyPrice text {
    display: block;
}
.everyPrice text:first-child {
    margin-right: 10rpx;
    font-size: 32rpx;
    color: #EA2243;
}
.everyPrice text:last-child {
    font-size: 20rpx;
    color: #999999;
}
.everyOtherInfo {
    display: flex;
    margin-top: 2rpx;
    justify-content: space-between;
    align-items: center;
}
.everyCountry {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.everyCountry image {
    display: block;
    width: 46rpx;
    height: 28rpx;
    margin-right: 10rpx;
}
.everyCountry text {
    display: block;
    font-size: 24rpx;
    color: #999999;
}
.everyOtherInfo > text {
    display: block;
    font-size: 24rpx;
    color: #333333;
}

.wareSelectElement {
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    width: 690rpx;
    padding: 30rpx;
    background: #ffffff;
    justify-content: space-between;
    align-items: center;
}
.selectAll {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.selectAll image {
    display: block;
    width: 28rpx;
    height: 28rpx;
    margin-right: 30rpx;
}
.selectAll text {
    display: block;
    font-size: 32rpx;
    color: #333333;
}
.selectAll text:last-child {
    margin-left: 64rpx;
}
.wareSelectElement > text {
    display: block;
    width: 200rpx;
    height: 80rpx;
    background: #EA2243;
    border-radius: 12rpx;
    line-height: 80rpx;
    font-size: 36rpx;
    color: #ffffff;
    text-align: center;
}

.wareSuccessModel {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background:rgba(0, 0, 0, .5);
    z-index: 10;
}
.wareSuccessBox {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 630rpx;
    height: 346rpx;
    background: #ffffff;
    border-radius: 10rpx;
    transform: translateY(-50%) translateX(-50%);
}
.wareSuccessBox > text:first-child {
    display: block;
    width: 540rpx;
    margin: 0 auto;
    margin-top: 40rpx;
    font-size: 40rpx;
    color: #333333;
    text-align: center;
}
.successText {
    width: 540rpx;
    margin: 0 auto;
    line-height: 56rpx;
    font-size: 36rpx;
    color: #999999;
}
.successText text {
    display: inline-block;
    color: #EA2243;
}
.wareSuccessBox > text:last-child {
    display: block;
    width: 100%;
    height: 110rpx;
    margin: 0 auto;
    margin-top: 20rpx;
    border-top: 2rpx solid #F2F2F2;
    line-height:110rpx;
    font-size: 36rpx;
    color: #EA2243;
    text-align: center;
}

.fix-iphonex-button::after {
    background: #ffffff;
} 

.noMore {
    display: flex;
    width: 100%;
    height: 80rpx;
    margin: 0 auto;
    justify-content: center;
    align-items: center;
}