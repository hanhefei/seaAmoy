/**pages/sellerBasic/orderManage/orderDetails**/
.orderElement {
    width: 100%;
}
.orderInfo {
    display: flex;
    width: 690rpx;
    padding: 30rpx 30rpx;
    margin-bottom: 16rpx;
    background: #ffffff;
    justify-content: space-between;
}
.orderInfo > image {
    display: block;
    width: 168rpx;
    height: 168rpx;
    border-radius: 16rpx;
}
.orderDescribe {
    width: 500rpx;
}
.orderTitle {
    overflow: hidden;
    display: -webkit-box;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #333333;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    text-overflow: ellipsis;
}
.orderModel {
    display: block;
    margin-top: 2rpx;
    font-size: 22rpx;
    color: #999999;
}
.orderPriceInfo {
    display: flex;
    margin-top: 4rpx;
    justify-content: space-between;
    align-items: center;
}
.orderPriceInfo text {
    font-size: 32rpx;
}
.orderPrice {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.orderPrice > text:first-child {
    display: block;
    margin-right: 10rpx;
    color: #EA2243;
}
.orderPrice > text:last-child {
    display: block;
    font-size: 20rpx;
    color: #999999;
}
.orderPriceInfo > text {
    display: block;
    color: #333333;
}

.expressElement {
    width: 720rpx;
    padding: 30rpx 0;
    padding-left: 30rpx;
    background: #ffffff;
}
.expressName {
    display: flex;
    padding-bottom: 30rpx;
    border-bottom: 2rpx solid #F2F2F2;
    justify-content: flex-start;
    align-items: center;
}
.expressName > image {
    display: block;
    width: 96rpx;
    height: 96rpx;
    margin-right: 30rpx;
    border-radius: 50%;
}
.expressNameInfo > text {
    display: block;
    font-size: 28rpx;
    color: #333333;
}
.expressPhone {
    display: flex;
    justify-content: flex-start;
    align-items: center;
}
.expressPhone text {
    display: block;
    font-size: 28rpx;
    color: #666666;
}
.expressPhone text:nth-child(1) {
    margin-right: 20rpx;
}
.expressPhone text:nth-child(2) {
    margin-right: 16rpx;
}
.expressPhone image {
    display: block;
    width: 12rpx;
    height: 22rpx;
}
.expressCode {
    display: flex;
    margin-top: 30rpx;
    justify-content: flex-start;
    align-items: center;
}
.expressCode text {
    display: block;
    font-size: 28rpx;
    color: #333333;
}
.expressCode text:first-child {
    margin-right: 20rpx;
}