.confirm {
    display: block;
    width: 690rpx;
    height: 80rpx;
    margin: 0 auto;
    margin-top: 60rpx;
    background: #EA2243;
    border-radius: 12rpx;
    line-height: 80rpx;
    font-size: 36rpx;
    color: #ffffff;
    text-align: center;
}

.cancel {
