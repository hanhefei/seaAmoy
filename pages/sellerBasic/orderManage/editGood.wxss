/**pages/sellerBasic/orderManage/editGood**/
.editText {
    width: 690rpx;
    margin-bottom: 16rpx;
    padding: 0 30rpx;
    background: #ffffff;
}
.editText text {
    display: block;
    margin-top: 30rpx;
    font-size: 32rpx;
    color: #333333;
}
.editText textarea {
    display: block;
    width: 646rpx;
    height: 172rpx;
    margin-top: 38rpx;
    margin-bottom: 30rpx;
    padding: 20rpx;
    border: 2rpx solid #DDDDDD;
    font-size: 28rpx;
    color: #666666;
}

.editNoText {
    width: 690rpx;
